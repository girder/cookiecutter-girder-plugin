{
    "name": "@girder/{{ cookiecutter.package_name }}",
    "version": "{{ cookiecutter.version }}",
    "description": "{{ cookiecutter.short_description }}",
    "homepage": "{{ cookiecutter.homepage }}",
    "bugs": {
      "url": "https://github.com/girder/girder/issues"
    },
    "license": "Apache-2.0",
    "repository": {
      "type": "git",
      "url": "git+https://github.com/girder/girder.git"
    },
    "type": "module",
    "files": [
      "dist",
      "package.json"
    ],
    "main": "./dist/girder-plugin-{{ cookiecutter.package_name }}.umd.cjs",
    "module": "./dist/girder-plugin-{{ cookiecutter.package_name }}.js",
    "exports": {
      ".": {
        "import": "./dist/girder-plugin-{{ cookiecutter.package_name }}.js",
        "require": "./dist/girder-plugin-{{ cookiecutter.package_name }}.umd.cjs"
      },
      "./style.css": "./dist/style.css",
      "./package.json": "./package.json"
    },
    "scripts": {
      "build": "vite build",
      "dev": "vite build --watch"
    },
    "author": "",
    "dependencies": {
      "@girder/core": "^5.0.0-beta.0"
    },
    "devDependencies": {
      "pug": "^3.0.2",
      "stylus": "^0.59.0",
      "vite": "^4.3.9"
    }
  }
